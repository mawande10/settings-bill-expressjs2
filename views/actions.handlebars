<div class="row">
    <h1>Actions</h1>
    <table >
        <tr>
            <th style="padding: 20px;">Type</th>
            <th>Price</th>
            <th>When</th>
        </tr>
        {{#each actions}}
        <tr>
            <td style="padding: 20px;">{{type}}</td>
            <td>{{cost}}</td>
            <td>{{timeframes}}</td>
        </tr>
        {{/each}}
    </table>
</div>

